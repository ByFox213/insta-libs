prefix="/usr/local"
exec_prefix="${prefix}"
libdir="${prefix}/lib"
includedir="${prefix}/include"

Name: NATS & NATS Streaming - C Client library
Description: A C client library for the NATS messaging system.
URL: https://github.com/nats-io/nats.c
Version: 3.10.0
Cflags: -I"${includedir}"
Libs: -L"${libdir}" -lnats
Libs.private: -L"${libdir}" -lnats 
